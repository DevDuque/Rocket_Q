<section>
    <h2>
        Create your own room
    </h2>
    <form action="/create-room" method="POST">
        <label class="sr-only" for="room-pass">Input the room code</label>
        <input type="password" name="password" id="room-pass" placeholder="Room code"/>
        <button>
            <img src="images/users-white.svg" alt="Join Room"/>
            Create room
        </button>
    </form>
</section>